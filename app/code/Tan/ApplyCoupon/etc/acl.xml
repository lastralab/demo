<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin" title="admin">
                <resource id="Tan_ApplyCoupon::coupon" title="coupon code link">
                        <resource id="Tan_ApplyCoupon::coupon_link" title="Coupon Code Links" translate="title"/>
                        <resource id="Tan_ApplyCoupon::apply_coupon_configuration" title="Coupon Code Link Configuration" translate="title"/>
                </resource>
                <resource id="Magento_Backend::stores">

                    <resource id="Magento_Backend::stores_settings">

                        <resource id="Magento_Config::config">

                            <resource id="Tan_ApplyCoupon::configuration" title="Tan Coupon Code Link Configuration" />

                        </resource>

                    </resource>

                </resource>
            </resource>
        </resources>
    </acl>
</config>
